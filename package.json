{
  "name": "@vaaski/create-node",
  "version": "2.0.1",
  "description": "Personal node.js/typescript boilerplate",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=14.16"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "nodemon.json",
    ".prettierrc",
    ".eslintrc.json",
    "tsconfig.json",
    "vue/**/*",
    "pm2*"
  ],
  "bin": {
    "create-node": "dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "ts-node src",
    "test": "echo \"no tests\"",
    "format": "prettier -w **/*.{vue,ts,js,json}",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaaski/create-node.git"
  },
  "keywords": [
    "boilerplate",
    "nodejs",
    "typescript"
  ],
  "author": "vaaski <admin@vaa.ski>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaaski/create-node/issues"
  },
  "homepage": "https://github.com/vaaski/create-node#readme",
  "devDependencies": {
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ava": "^12.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "enquirer": "github:bitjson/enquirer#typings",
    "execa": "^5.1.1",
    "fs-jetpack": "^4.3.0",
    "ora": "^5.4.1"
  }
}
